import org.springframework.stereotype.Repository;

@Repository("dao")
public class EmployeeDao {

	@Autowired
	private JdbcTemplate jdbcTemp;
	
	String sql = "INSERT INTO EMPLOYEE (id, name, city) VALUES (1, 'Rohan', 'Delhi') ";
	
	public void input() {
		
		
		
		jdbcTemp.update(sql);
	}
	
	
}
